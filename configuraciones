# net-tools
para los comandos basicos como ifconfig

# Intalacion de Teamviwer 32 y 64
dpkg --add-architecture i386
apt-get update
aptitude install libdbus-1-3:i386 libc6:i386 libgcc1:i386 libasound2:i386 libfreetype6:i386 zlib1g:i386 libsm6:i386 libxdamage1:i386 libxext6:i386 libxfixes3:i386 libxrandr2:i386 libxrender1:i386 libxtst6:i386 libjpeg62:i386 libxinerama1:i386 libexpat1:i386 libfontconfig1:i386
dpkg -i teamviewer_12.0.76279_i386.deb
